using System;
using System.Globalization;

namespace Primeiro___Curso_de_LÃ³gica {
    class Program     {
        static void Main(string[] args)
        {
            int X = int.Parse(Console.ReadLine());

            for (int i = 1; i <= X; i += 2)
            {
                Console.WriteLine(i);
            }
        }
    }
}
