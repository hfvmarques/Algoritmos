using System;
using System.Globalization;

namespace Primeiro___Curso_de_LÃ³gica {
    class Program     {
        static void Main(string[] args)
        {
            int N = int.Parse(Console.ReadLine());

            for(int i = 2; i <= N; i += 2)
            {
                Console.WriteLine(i + "^2 = " + Math.Pow(i, 2));
            }
        }
    }
}
