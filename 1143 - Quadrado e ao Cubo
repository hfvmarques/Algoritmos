using System;
using System.Globalization;

namespace Primeiro___Curso_de_LÃ³gica {
    class Program     {
        static void Main(string[] args)
        {
            int N = int.Parse(Console.ReadLine());

            for (int i = 1; i <= N; i++)
            {
                Console.WriteLine(i + " " + (Math.Pow(i, 2)) + " " + (Math.Pow(i, 3)));
            }
        }
    }
}
