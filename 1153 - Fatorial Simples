using System;
using System.Globalization;

namespace Primeiro___Curso_de_LÃ³gica {
    class Program     {
        static void Main(string[] args)
        {
            int N = int.Parse(Console.ReadLine());
            int fatorial = 1;

            for (int i=N; i>=1; i--)
            {
                fatorial *= i;
            }
            Console.WriteLine(fatorial);
        }
    }
}
